{
    "name": "veda/utils",
    "description": "a set of useful php utils",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ketu.lai",
            "email": "ketu.lai@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Veda\\Utils\\": "src/Veda/Utils"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veda\\Utils\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
